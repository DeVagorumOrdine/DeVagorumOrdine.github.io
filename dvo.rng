               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(book edge) </a:documentation>
               <value>hc:BottomEdge</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(bottom edgetail edgelower edge) </a:documentation>
               <value>hc:ForeEdge</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(fore-edge) </a:documentation>
               <value>hc:FrontCover</value>
