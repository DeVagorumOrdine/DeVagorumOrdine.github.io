                  <oneOrMore>
                     <choice>
                        <value>hc:FlushLeft</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                        <value>hc:FlushRight</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                        <value>hc:Centered</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                        <value>hc:Justified</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                        <value>hc:VerticalSpaceBefore</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                        <value>hc:VerticalSpaceAfter</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                        <value>hc:FirstLineIndent</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(first-line-indented) A textual visual item whose first line is left-indented, usually corresponding to a paragraph as a semantic-logical unit.</a:documentation>
                        <value>hc:HangingIndent</value>
