         <ref name="teiatt.global.attribute.n"/>
         <ref name="teiatt.global.attribute.xmllang"/>
         <ref name="teiatt.global.attribute.xmlbase"/>
         <ref name="teiatt.global.attribute.xmlspace"/>
         <ref name="teiatt.global.rendition.attribute.rend"/>
         <ref name="teiatt.global.rendition.attribute.style"/>
         <ref name="teiatt.global.linking.attribute.corresp"/>
         <ref name="teiatt.global.linking.attribute.synch"/>
         <ref name="teiatt.global.linking.attribute.sameAs"/>
         <ref name="teiatt.global.linking.attribute.copyOf"/>
         <ref name="teiatt.global.linking.attribute.next"/>
         <ref name="teiatt.global.linking.attribute.prev"/>
         <ref name="teiatt.global.linking.attribute.exclude"/>
         <ref name="teiatt.global.linking.attribute.select"/>
         <ref name="teiatt.global.facs.attribute.facs"/>
         <ref name="teiatt.global.change.attribute.change"/>
         <ref name="teiatt.global.responsibility.attribute.cert"/>
         <ref name="teiatt.global.responsibility.attribute.resp"/>
         <ref name="teiatt.global.source.attribute.source"/>
         <ref name="teiatt.timed.attributes"/>
         <ref name="teiatt.editLike.attributes"/>
         <ref name="teiatt.dimensions.attribute.quantity"/>
         <ref name="teiatt.dimensions.attribute.precision"/>
         <ref name="teiatt.dimensions.attribute.scope"/>
         <ref name="teiatt.ranging.attribute.atLeast"/>
         <ref name="teiatt.ranging.attribute.atMost"/>
         <ref name="teiatt.ranging.attribute.min"/>
         <ref name="teiatt.ranging.attribute.max"/>
         <ref name="teiatt.ranging.attribute.confidence"/>
         <optional>
            <attribute name="rendition">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <list>
                  <oneOrMore>
                     <choice>
                        <value>hc:BrokenOff</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(broken off) </a:documentation>
                        <value>hc:Burnt</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(burnt) </a:documentation>
                        <value>hc:ChemicalReagent</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(damaged by chemical reagent) </a:documentation>
                        <value>hc:CutOff</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(cut off) </a:documentation>
                        <value>hc:Erased</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(erased) </a:documentation>
                        <value>hc:Faded</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(faded) </a:documentation>
                        <value>hc:Illegible</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(illegible) </a:documentation>
                        <value>hc:Lost</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(lost) </a:documentation>
                        <value>hc:Mould</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(mould) </a:documentation>
                        <value>hc:Overpainted</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(overpainted) </a:documentation>
                        <value>hc:PastedOver</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(pasted over) </a:documentation>
                        <value>hc:PeeledOff</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(peeled off) </a:documentation>
                        <value>hc:Perforated</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(perforated) </a:documentation>
                        <value>hc:Polluted</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(polluted) </a:documentation>
                        <value>hc:RubbedOff</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(rubbed off) </a:documentation>
                        <value>hc:Stained</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(stained) </a:documentation>
                        <value>hc:TornOff</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(torn off) </a:documentation>
