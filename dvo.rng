                        <value>hc:LineDrawing</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(line drawing) </a:documentation>
                        <value>hc:Map</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(map) </a:documentation>
                        <value>hc:VerticalLine</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(vertical linevertical rule) </a:documentation>
                     </choice>
                  </oneOrMore>
               </list>
            </attribute>
