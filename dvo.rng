                  <oneOrMore>
                     <choice>
                        <value>hc:AlignmentLeft</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(item with left-aligned content) A visual item whose content is aligned with the left margin.</a:documentation>
                        <value>hc:AlignmentRight</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(item with right-aligned content) A visual item whose content is aligned with the right margin.</a:documentation>
                        <value>hc:AlignmentCentered</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(item with centered content) A visual item whose content is centered.</a:documentation>
                        <value>hc:AlignmentBottom</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                        <value>hc:AlignmentTop</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                        <value>hc:SelfAlignmentBottom</value>
