                  <zeroOrMore>
                     <ref name="teimodel.global"/>
                  </zeroOrMore>
               </group>
            </oneOrMore>
            <zeroOrMore>
               <group>
                  <ref name="teimodel.common"/>
                  <zeroOrMore>
                     <ref name="teimodel.global"/>
                  </zeroOrMore>
               </group>
            </zeroOrMore>
         </group>
         <ref name="teiatt.global.attributes"/>
         <empty/>
      </element>
