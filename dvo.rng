                  id="tei_hei-note-note-index-constraint-report-11">
            <rule context="tei:note">
               <sch:report xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns:hei="https://digi.ub.uni-heidelberg.de/schema/tei/heiEDITIONS"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
                           test="@ana='hc:IndexNote' and not(parent::tei:person or parent::tei:place or parent::tei:org)">The element "note" with value "hc:IndexNote" in @ana needs to be a child of one of the elements "person", "place" or "org"!</sch:report>
            </rule>
         </pattern>
         <ref name="teiatt.global.attribute.xmlid"/>
         <ref name="teiatt.global.attribute.n"/>
         <ref name="teiatt.global.attribute.xmllang"/>
         <ref name="teiatt.global.attribute.xmlbase"/>
         <ref name="teiatt.global.attribute.xmlspace"/>
         <ref name="teiatt.global.rendition.attribute.rend"/>
         <ref name="teiatt.global.rendition.attribute.style"/>
         <ref name="teiatt.global.rendition.attribute.rendition"/>
         <ref name="teiatt.global.linking.attribute.corresp"/>
         <ref name="teiatt.global.linking.attribute.synch"/>
         <ref name="teiatt.global.linking.attribute.sameAs"/>
         <ref name="teiatt.global.linking.attribute.copyOf"/>
         <ref name="teiatt.global.linking.attribute.next"/>
         <ref name="teiatt.global.linking.attribute.prev"/>
         <ref name="teiatt.global.linking.attribute.exclude"/>
         <ref name="teiatt.global.linking.attribute.select"/>
         <ref name="teiatt.global.facs.attribute.facs"/>
         <ref name="teiatt.global.change.attribute.change"/>
         <ref name="teiatt.global.responsibility.attribute.cert"/>
         <ref name="teiatt.global.responsibility.attribute.resp"/>
         <ref name="teiatt.global.source.attribute.source"/>
         <ref name="teiatt.placement.attributes"/>
         <ref name="teiatt.pointing.attributes"/>
         <ref name="teiatt.written.attributes"/>
         <ref name="teiatt.anchoring.attributes"/>
         <ref name="teiatt.hei-zonemembership.attributes"/>
         <optional>
            <attribute name="ana">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <list>
                  <oneOrMore>
                     <choice>
                        <value>hc:NotaBene</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(nota) </a:documentation>
                        <value>hc:Manicula</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(manicule) </a:documentation>
                        <value>hc:AuthorialNote</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(authorial note) An annotation originating from the original author of the expression of which it is a part.</a:documentation>
                        <value>hc:BiblicalNote</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(annotation about biblical sources) An annotation about the biblical sources of a passage.</a:documentation>
                        <value>hc:Comment</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(comment) A single commenting annotation about content-related aspects of an expression part.</a:documentation>
                        <value>hc:CompiledBibliographyPlain</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(bibliography compiled to plain text) </a:documentation>
                        <value>hc:CopyingNote</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(note about copying) An annotation originating from the copyist of the expression referring to the copying process, the findings in the source or other things connected with the making of the copy.</a:documentation>
                        <value>hc:EditorialContent</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(editorial content) </a:documentation>
                        <value>hc:EditorialNote</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(editorial note) An annotation or comment originating from the copyist, editor or another person involved in some way, but not from the author of the expression himself.</a:documentation>
                        <value>hc:Endnote</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(end-note) An annotation or reference placed (typically together with other annotations or references of its kind) at the end of a publication or a publication part.</a:documentation>
                        <value>hc:ExemplarNote</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(an annotation present in the physical text exemplar) An annotation already present in the physical carrier of the edited expression and not originating in the making of the edition only.</a:documentation>
                        <value>hc:FontesNote</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(annotation about sources) An annotation about the sources of a passage.</a:documentation>
                        <value>hc:Footnote</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(footnote) A note, reference, or additional piece of information [placed] at the bottom of a page, used to explain or comment on something in the main body of the text on the same page. Oxford English Dictionary Online 2001–footnote, n. <a xmlns="http://www.w3.org/1999/xhtml"
                              href="https://www.oed.com/view/Entry/72724">https://www.oed.com/view/Entry/72724</a>
                        </a:documentation>
                        <value>hc:Gloss</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(gloss) A historical (premodern) annotation.</a:documentation>
                        <value>hc:GrammaticalGloss</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(grammatical gloss) A gloss explaining grammatical aspects of a word or a word group.</a:documentation>
                        <value>hc:IndexNote</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(index note) A note which is part of a index entry.</a:documentation>
                        <value>hc:InterlinearGloss</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(interlinear gloss) A gloss in an interlinear line.</a:documentation>
                        <value>hc:InterpretativeGloss</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(interpretative gloss) A gloss explaining content-related aspects of a textual passage which go beyond a purely lexical explanation.</a:documentation>
                        <value>hc:LexicalGloss</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(lexical gloss) A gloss providing a lexical explanation of a word or a word group, typically using synonyms or paraphrases in the same language as the glossed text.</a:documentation>
                        <value>hc:LociNote</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(annotation about textual loci (sources, parallels or reception)) An annotation about textual loci (sources, parallels or reception) of a passage.</a:documentation>
                        <value>hc:MarginalGloss</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(marginal gloss) A gloss outside of the type area or script area.</a:documentation>
                        <value>hc:NamedEntityGloss</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(named entity gloss) A gloss explaining an individual which is referred to by name or another reference in the glossed text.</a:documentation>
                        <value>hc:ResponsiveGloss</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(responsive gloss) Gloss reacting in a personal way to the content.</a:documentation>
                        <value>hc:Scholium</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(scholium) An explanatory note or comment; spec[ifically] an ancient exegetical note or comment upon a passage in a Greek or Latin author Oxford English Dictionary 2198914 631; cf. also a short explanation of difficult (regarding the language or the content) text passages in the margin of a papyrus column or a manuscript page Hönscheid 2003 (translated), the physical placement however being not essential.</a:documentation>
                        <value>hc:SimiliaNote</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(annotation about parallels) An annotation about parallels or similia of a passage.</a:documentation>
                        <value>hc:TestimoniaNote</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(annotation about reception) An annotation about the reception or younger attestations of a passage.</a:documentation>
                        <value>hc:TextCriticalNote</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(text-critical note) An annotation about the constitution of the text or about textual variants.</a:documentation>
                        <value>hc:TranscriptionNote</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(transcription note) An annotation about the findings and phenomena of the source or about the transfer of the text from the source, typically of palaeographical, palaeotypographical or codicological nature or about legibility.</a:documentation>
                        <value>hc:TranslationGloss</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(translation gloss) A gloss providing a translation of a word or a word group.</a:documentation>
                        <value>hc:WitnessesNote</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(note about consulted witnesses) An annotation about the textual witnesses consulted for the constitution of a part of the edited expression, typically in the form of a list of sigla.</a:documentation>
                     </choice>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="teinoteGrp">
      <element name="noteGrp">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains a group of notes [3.9.1.1. Encoding Grouped Notes]</a:documentation>
         <group>
            <zeroOrMore>
               <ref name="teidesc"/>
            </zeroOrMore>
            <oneOrMore>
               <choice>
                  <ref name="teinote"/>
                  <ref name="teinoteGrp"/>
               </choice>
            </oneOrMore>
         </group>
         <ref name="teiatt.global.attributes"/>
         <ref name="teiatt.placement.attributes"/>
         <ref name="teiatt.pointing.attributes"/>
         <ref name="teiatt.written.attributes"/>
         <ref name="teiatt.anchoring.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="teiindex">
      <element name="index">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(index entry) marks a location to be indexed for whatever purpose. [3.9.2. Index Entries]</a:documentation>
         <zeroOrMore>
            <group>
               <ref name="teiterm"/>
               <optional>
                  <ref name="teiindex"/>
               </optional>
            </group>
         </zeroOrMore>
         <ref name="teiatt.global.attributes"/>
         <ref name="teiatt.spanning.attributes"/>
         <optional>
            <attribute name="indexName">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">a single word which follows the rules defining a legal XML name (see ), supplying a name to specify which index (of several) the index entry belongs to.</a:documentation>
               <data type="Name"/>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="teimedia">
      <element name="media">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">indicates the location of any form of external media such as an audio or video clip etc. [3.10. Graphics and Other Non-textual Components]</a:documentation>
         <zeroOrMore>
            <ref name="teimodel.descLike"/>
         </zeroOrMore>
         <ref name="teiatt.global.attributes"/>
         <ref name="teiatt.media.attribute.width"/>
         <ref name="teiatt.media.attribute.height"/>
         <ref name="teiatt.media.attribute.scale"/>
         <ref name="teiatt.resourced.attributes"/>
         <ref name="teiatt.declaring.attributes"/>
         <ref name="teiatt.timed.attributes"/>
         <attribute name="mimeType">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(MIME media type) specifies the applicable multimedia internet mail extension (MIME) media type</a:documentation>
            <list>
               <oneOrMore>
                  <data type="token">
                     <param name="pattern">[^\p{C}\p{Z}]+</param>
                  </data>
               </oneOrMore>
            </list>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="teigraphic">
      <element name="graphic">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(graphic) Strukturierte Angabe der Verknüpfung zu einer digitalen Abbildung des visuellen Objekts, dessen Inhalt durch das Elternelement repräsentiert, dokumentiert und erschlossen wird. [3.10. Graphics and Other Non-textual Components 11.1. Digital Facsimiles]</a:documentation>
         <zeroOrMore>
            <ref name="teimodel.descLike"/>
         </zeroOrMore>
         <ref name="teiatt.global.attributes"/>
         <ref name="teiatt.media.attribute.width"/>
         <ref name="teiatt.media.attribute.height"/>
         <ref name="teiatt.media.attribute.scale"/>
         <ref name="teiatt.declaring.attributes"/>
         <optional>
            <attribute name="mimeType">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(MIME media type) gibt den Medientyp des verknüpften Bildes als MIME media type an.</a:documentation>
               <list>
                  <oneOrMore>
                     <data type="token">
                        <param name="pattern">[^\p{C}\p{Z}]+</param>
                     </data>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <attribute name="url">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">gibt die URL einer digitalen Abbildung an.</a:documentation>
            <data type="anyURI"/>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="teibinaryObject">
      <element name="binaryObject">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">provides encoded binary data representing an inline graphic, audio, video or other object. [3.10. Graphics and Other Non-textual Components]</a:documentation>
         <text/>
         <ref name="teiatt.global.attributes"/>
         <ref name="teiatt.media.attributes"/>
         <ref name="teiatt.timed.attributes"/>
         <optional>
            <attribute name="encoding">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">The encoding used to encode the binary data. If not specified, this is assumed to be <a xmlns="http://www.w3.org/1999/xhtml"
                     href="http://en.wikipedia.org/wiki/Base64">Base64</a>.</a:documentation>
               <list>
                  <oneOrMore>
                     <data type="token">
                        <param name="pattern">[^\p{C}\p{Z}]+</param>
                     </data>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
