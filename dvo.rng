   </define>
   <define name="teiauthority">
      <element name="authority">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(release authority) supplies the name of a person or other agency responsible for making a work available, other than a publisher or distributor. [2.2.4. Publication, Distribution, Licensing, etc.]</a:documentation>
         <ref name="teimacro.phraseSeq.limited"/>
         <ref name="teiatt.global.attributes"/>
         <ref name="teiatt.canonical.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="teiidno">
      <element name="idno">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(identifier) supplies any form of identifier used to identify some object, such as a bibliographic item, a person, a title, an organization, etc. in a standardized way. [13.3.1. Basic Principles 2.2.4. Publication, Distribution, Licensing, etc. 2.2.5. The Series Statement 3.12.2.4. Imprint, Size of a Document, and Reprint Information]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="teimodel.gLike"/>
               <ref name="teiidno"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tei_hei-idno-idno-gnd-constraint-report-21">
            <rule context="tei:idno">
               <sch:report xmlns:xi="http://www.w3.org/2001/XInclude"
                           xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns:hei="https://digi.ub.uni-heidelberg.de/schema/tei/heiEDITIONS"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
                           test="@ana='hc:GNDURI' and not(matches(., '^http://d-nb.info/gnd/1[01]?\d{7}[0-9X]|[47]\d{6}-\d|[1-9]\d{0,7}-[0-9X]|3\d{7}[0-9X]$'))">Not a valid GND URI!</sch:report>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tei_hei-idno-idno-orcid-constraint-report-22">
            <rule context="tei:idno">
               <sch:report xmlns:xi="http://www.w3.org/2001/XInclude"
                           xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns:hei="https://digi.ub.uni-heidelberg.de/schema/tei/heiEDITIONS"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
                           test="@ana='hc:ORCIDURI' and not(matches(., '^https://orcid.org/[0-9]{4}-[0-9]{4}-[0-9]{4}-[0-9]{4}$'))">Not a valid ORCID ID!</sch:report>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tei_hei-idno-idno-urn-constraint-report-23">
            <rule context="tei:idno">
               <sch:report xmlns:xi="http://www.w3.org/2001/XInclude"
                           xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns:hei="https://digi.ub.uni-heidelberg.de/schema/tei/heiEDITIONS"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
                           test="@ana='hc:URN' and not(starts-with(., 'urn:'))">A URN must start with the prefix "urn" followed by a colon!</sch:report>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tei_hei-idno-idno-doi-constraint-report-24">
            <rule context="tei:idno">
               <sch:report xmlns:xi="http://www.w3.org/2001/XInclude"
                           xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns:hei="https://digi.ub.uni-heidelberg.de/schema/tei/heiEDITIONS"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
                           test="@ana='hc:DOI' and not(starts-with(., 'https://doi.org/'))">A DOI URL must start with "https://doi.org/"!</sch:report>
            </rule>
         </pattern>
         <ref name="teiatt.global.attribute.xmlid"/>
         <ref name="teiatt.global.attribute.n"/>
         <ref name="teiatt.global.attribute.xmllang"/>
         <ref name="teiatt.global.attribute.xmlbase"/>
         <ref name="teiatt.global.attribute.xmlspace"/>
         <ref name="teiatt.global.rendition.attribute.rend"/>
         <ref name="teiatt.global.rendition.attribute.style"/>
         <ref name="teiatt.global.rendition.attribute.rendition"/>
         <ref name="teiatt.global.linking.attribute.corresp"/>
         <ref name="teiatt.global.linking.attribute.synch"/>
         <ref name="teiatt.global.linking.attribute.sameAs"/>
         <ref name="teiatt.global.linking.attribute.copyOf"/>
         <ref name="teiatt.global.linking.attribute.next"/>
         <ref name="teiatt.global.linking.attribute.prev"/>
         <ref name="teiatt.global.linking.attribute.exclude"/>
         <ref name="teiatt.global.linking.attribute.select"/>
         <ref name="teiatt.global.facs.attribute.facs"/>
         <ref name="teiatt.global.change.attribute.change"/>
         <ref name="teiatt.global.responsibility.attribute.cert"/>
         <ref name="teiatt.global.responsibility.attribute.resp"/>
         <ref name="teiatt.global.source.attribute.source"/>
         <ref name="teiatt.sortable.attributes"/>
         <ref name="teiatt.datable.attributes"/>
         <optional>
            <attribute name="ana">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <list>
                  <oneOrMore>
                     <choice>
                        <value>hc:DigitalPublicationIdentifier</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(digital publication identifier) </a:documentation>
                        <value>hc:DOI</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(DOI) </a:documentation>
                        <value>hc:EditorialSiglum</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(editorial siglum) </a:documentation>
                        <value>hc:GNDURI</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(GND URI) </a:documentation>
                        <value>hc:ISBN</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(ISBN) </a:documentation>
                        <value>hc:ISSN</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(ISSN) </a:documentation>
                        <value>hc:K10plusPPN</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(K10plus-PPN) </a:documentation>
                        <value>hc:ORCIDURI</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(ORCID URI) </a:documentation>
                        <value>hc:PrintPublicationIdentifier</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(print publication identifier) </a:documentation>
                        <value>hc:Shelfmark</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(shelfmark) </a:documentation>
                        <value>hc:URN</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(URN) </a:documentation>
                        <value>hc:ZDBID</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(ZDB-ID) Identification number of the database Zeitschriftendatenbank (ZDB) which—with the prefixed https://ld.zdb-services.de/resource/— can be used to invoke the corresponding title page in the ZDB catalogue (cf. Zeitschriftendatenbank 2019).</a:documentation>
                     </choice>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <empty/>
      </element>
