   <define name="teilist">
      <element name="list">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(list) contains any sequence of items organized as a list. [3.8. Lists]</a:documentation>
         <group>
            <zeroOrMore>
               <choice>
                  <ref name="teimodel.divTop"/>
                  <ref name="teimodel.global"/>
                  <zeroOrMore>
                     <ref name="teidesc"/>
                  </zeroOrMore>
               </choice>
            </zeroOrMore>
            <choice>
               <oneOrMore>
                  <group>
                     <ref name="teiitem"/>
                     <zeroOrMore>
                        <ref name="teimodel.global"/>
                     </zeroOrMore>
                  </group>
               </oneOrMore>
               <group>
                  <optional>
                     <ref name="teiheadLabel"/>
                  </optional>
                  <optional>
                     <ref name="teiheadItem"/>
                  </optional>
                  <oneOrMore>
                     <group>
                        <ref name="teilabel"/>
                        <zeroOrMore>
                           <ref name="teimodel.global"/>
                        </zeroOrMore>
                        <ref name="teiitem"/>
                        <zeroOrMore>
                           <ref name="teimodel.global"/>
                        </zeroOrMore>
                     </group>
                  </oneOrMore>
               </group>
            </choice>
            <zeroOrMore>
               <group>
                  <ref name="teimodel.divBottom"/>
                  <zeroOrMore>
                     <ref name="teimodel.global"/>
                  </zeroOrMore>
               </group>
